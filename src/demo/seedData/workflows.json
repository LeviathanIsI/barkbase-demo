                "groups": [{
                  "id": "group-1",
                  "logic": "and",
                  "conditions": [
                    { "field": "amount_paid", "operator": "is_greater_than", "value": 0 },
                    { "field": "status", "operator": "is_none_of", "values": ["paid"] }
                  ]
                }],
                "groupLogic": "or"
              }
            },
            {
              "id": "unpaid",
              "name": "Still Unpaid",
              "order": 2,
              "conditions": {
                "groups": [{
                  "id": "group-1",
                  "logic": "and",
                  "conditions": [
                    { "field": "amount_paid", "operator": "is_equal_to", "value": 0 }
                  ]
                }],
                "groupLogic": "or"
              }
            },
            {
              "id": "none-matched",
              "name": "None matched",
              "order": 999,
              "isDefault": true,
              "conditions": null
            }
          ]
        },
        "position": 2
