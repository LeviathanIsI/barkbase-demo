        "config": {
          "branches": [
            {
              "id": "paid",
              "name": "Paid",
              "order": 0,
              "conditions": {
                "groups": [{
                  "id": "group-1",
                  "logic": "and",
                  "conditions": [
                    { "field": "status", "operator": "is_any_of", "values": ["paid"] }
