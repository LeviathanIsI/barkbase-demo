  {
    "id": "workflow-001",
    "recordId": "workflow-001",
    "tenantId": "demo-tenant",
    "name": "Vaccination Reminder",
    "description": "Send SMS and email reminders when pet vaccinations are expiring soon",
    "objectType": "pet",
    "status": "active",
    "triggerType": "event",
    "triggerConfig": {
      "event": "pet.vaccination_expiring",
      "daysBeforeExpiry": 30
    },
    "entryCondition": {
      "triggerType": "event",
      "eventType": "pet.vaccination_expiring",
      "filterConfig": null,
      "scheduleConfig": null
    },
    "enrolledTotal": 127,
    "enrolledLast7Days": 12,
    "completedTotal": 98,
    "failedTotal": 2,
    "createdAt": { "$daysFromNow": -45 },
    "updatedAt": { "$daysFromNow": -2 },
    "createdBy": "staff-001",
    "steps": [
      {
        "id": "step-1",
        "type": "action",
        "actionType": "send_sms",
        "name": "Send SMS Reminder",
        "config": {
          "template": "vaccination_reminder",
          "message": "Hi {{owner.firstName}}, {{pet.name}}'s {{vaccination.type}} vaccination expires on {{vaccination.expiryDate}}. Book an appointment today!"
        },
        "position": 0
      },
      {
        "id": "step-2",
        "type": "wait",
        "name": "Wait 3 Days",
        "config": {
          "duration": 3,
          "unit": "days"
        },
        "position": 1
      },
      {
        "id": "step-3",
        "type": "determinator",
        "name": "Check Booking Status",
        "config": {
          "branches": [
            {
              "id": "booked",
              "name": "Booked Appointment",
              "order": 0,
              "conditions": {
                "groups": [{
                  "id": "group-1",
                  "logic": "and",
                  "conditions": [
                    { "field": "vaccination_status", "operator": "is_any_of", "values": ["current"] }
                  ]
                }],
                "groupLogic": "or"
              }
            },
            {
              "id": "not-booked",
              "name": "No Booking Yet",
              "order": 1,
              "conditions": {
                "groups": [{
                  "id": "group-1",
                  "logic": "and",
                  "conditions": [
                    { "field": "vaccination_status", "operator": "is_any_of", "values": ["expiring", "expired"] }
                  ]
                }],
                "groupLogic": "or"
              }
            },
            {
              "id": "none-matched",
              "name": "None matched",
              "order": 999,
              "isDefault": true,
              "conditions": null
            }
          ]
        },
        "position": 2
      },
      {
        "id": "step-4",
        "type": "action",
        "actionType": "send_email",
        "name": "Send Confirmation",
        "config": {
          "template": "vaccination_appointment_confirmed",
          "subject": "Appointment Confirmed for {{pet.name}}"
        },
        "position": 0,
        "parentStepId": "step-3",
        "branchId": "booked"
      },
      {
        "id": "step-5",
        "type": "action",
        "actionType": "send_email",
        "name": "Send Urgent Follow-up",
        "config": {
          "template": "vaccination_reminder_followup",
          "subject": "Urgent: {{pet.name}}'s vaccination is expiring soon"
        },
        "position": 0,
        "parentStepId": "step-3",
        "branchId": "not-booked"
      },
      {
        "id": "step-6",
        "type": "wait",
        "name": "Wait 4 Days",
        "config": {
          "duration": 4,
          "unit": "days"
        },
        "position": 1,
        "parentStepId": "step-3",
        "branchId": "not-booked"
      },
      {
        "id": "step-7",
        "type": "action",
        "actionType": "create_task",
        "name": "Create Follow-up Task",
        "config": {
          "taskType": "follow_up",
          "title": "Call owner about expiring vaccination for {{pet.name}}",
          "priority": "high"
        },
        "position": 2,
        "parentStepId": "step-3",
        "branchId": "not-booked"
      }
    ]
  },
